------------------------------------------------------------
if(i==j){
  h = i +j ; 
}
else h = i - j ; 
------------------------------------------------------------

R1 = i, R2 = j, R3 = h

bne R1, R2, Else           ; =!
add R3, R1, R2             ; +
j Skip                     ; GOTO 

Else : 
sub R3, R1, R2              ; -

Skip: 
